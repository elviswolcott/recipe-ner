ican"],"@id":"https://veganheaven.org/recipe/peanut-butter-stuffed-dates-chocolate/#recipe","isPartOf":{"@id":"https://veganheaven.org/recipe/peanut-butter-stuffed-dates-chocolate/#article"},"mainEntityOfPage":"https://veganheaven.org/recipe/peanut-butter-stuffed-dates-chocolate/#webpage"}]}